{
  "private": true,
  "type": "module",
  "name": "skyline-overlay",
  "version": "3.1.1",
  "description": "A modern customizable horizon FFXIV miniparse overlay.",
  "license": "Apache-2.0",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.net)",
  "scripts": {
    "eslint": "eslint --ext \".jsx,.js,.tsx,.ts\" \"./src\"",
    "prettier": "prettier --write .",
    "typecheck": "tsc",
    "bundle": "vite build",
    "serve": "vite",
    "script:genIconIdx": "node \"./scripts/genIconIdx.mjs\"",
    "script:genMeta": "node \"./scripts/genMeta.mjs\"",
    "script:modSource": "node \"./scripts/modSource.mjs\"",
    "script:removeSVG": "node \"./scripts/removeSVG.mjs\"",
    "build": "cross-env NODE_ENV=production run-s script:genIconIdx script:genMeta eslint typecheck bundle script:modSource script:removeSVG",
    "dev": "cross-env NODE_ENV=development run-s script:genIconIdx script:genMeta serve",
    "start": "serve"
  },
  "dependencies": {
    "@sentry/react": "~6.16.1",
    "@sentry/tracing": "~6.16.1",
    "classnames": "~2.3.1",
    "ffxiv-overlay-api": "~4.1.4",
    "mobx": "~6.3.10",
    "mobx-react-lite": "~3.2.3",
    "normalize.css": "~8.0.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "devDependencies": {
    "@babel/core": "~7.16.7",
    "@babel/preset-typescript": "~7.16.7",
    "@types/node": "~16.11.17",
    "@types/react": "~17.0.38",
    "@types/react-dom": "~17.0.11",
    "@typescript-eslint/eslint-plugin": "~5.8.1",
    "@typescript-eslint/parser": "~5.8.1",
    "@vitejs/plugin-react": "~1.1.3",
    "chalk": "~5.0.0",
    "cross-env": "~7.0.3",
    "eslint": "~8.6.0",
    "eslint-plugin-react": "~7.28.0",
    "eslint-plugin-react-hooks": "~4.3.0",
    "glob": "~7.2.0",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.5.1",
    "rollup-plugin-visualizer": "~5.5.2",
    "sass": "~1.45.2",
    "serve": "~13.0.2",
    "typescript": "~4.5.4",
    "vite": "~2.7.10",
    "vite-plugin-html-env": "~1.0.3",
    "vite-plugin-svgr": "~0.6.0"
  }
}
