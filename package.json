{
  "private": true,
  "name": "skyline-overlay",
  "version": "4.4.0",
  "versionCode": "Awa Subaru",
  "description": "A modern customizable horizon FFXIV miniparse overlay.",
  "license": "Apache-2.0",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.net)",
  "scripts": {
    "eslint": "eslint ./src",
    "typecheck": "tsc",
    "bundle": "vite build",
    "serve": "vite",
    "script:genIconIdx": "node \"./scripts/genIconIdx.mjs\"",
    "script:genMeta": "node \"./scripts/genMeta.mjs\"",
    "script:removeTrash": "node \"./scripts/removeTrash.mjs\"",
    "build": "cross-env NODE_ENV=production run-s script:genIconIdx script:genMeta typecheck eslint bundle script:removeTrash",
    "analyze": "cross-env NODE_ENV=production ENABLE_ANALYZER=1 run-s script:genIconIdx script:genMeta bundle",
    "dev": "cross-env NODE_ENV=development VITE_GA_ID=TEST run-s script:genIconIdx script:genMeta serve"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "clsx": "^2.1.1",
    "ffxiv-overlay-api": "^4.8.0",
    "immer": "^11.1.3",
    "normalize.css": "^8.0.1",
    "react": "^19.2.3",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.2.3",
    "react-redux": "^9.2.0",
    "stable-hash": "^0.0.6",
    "workbox-window": "^7.4.0"
  },
  "devDependencies": {
    "@dsrca/config": "^5.1.0",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "chalk": "^5.6.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "glob": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "sass": "^1.97.2",
    "typescript": "~5.8.3",
    "vite": "^7.3.0",
    "vite-bundle-analyzer": "^1.3.2",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-svgr": "^4.5.0"
  }
}
